{
  "name": "server",
  "version": "1.0.0",
  "description": "Mnemosyne quotes app server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test:setup": "node src/__tests__/setup-test-db.mjs",
    "test": "npm run test:setup && vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
